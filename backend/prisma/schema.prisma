generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Contato {
  id        Int      @id @default(autoincrement())
  nome      String
  email     String
  mensagem  String
  createdAt DateTime @default(now())
}

model Projeto {
  id     Int      @id @default(autoincrement())
  nome   String
  img    String
  tags   String[]
  link   String
}